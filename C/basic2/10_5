#include <stdio.h>

int main()
{
    //배열 전달은 포인터로 전달하는게 제일 편함.
    //1차원 배열의 경우, 배열의 이름을 포인터변수로 받아주면, 편함.
void print_ary(int *pa);

    int ary[5] = {1, 2, 3, 4, 5};
    print_ary(ary);
    return 0;
}

void print_ary(int *pa)
{
    for ( int i = 0; i < 5; i++)
    {
        printf("%d", pa[i]);
    }
    
}
