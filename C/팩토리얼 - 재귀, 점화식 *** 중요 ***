#include <stdio.h>
long long factorial(long long n)
{
    if (n==1)
    {
        return 1;
    }
        return n * factorial(n-1);
}        
int main()
{
    int n=0;
    scanf("%d", &n);
    printf("\n%d\n", factorial(n));




    int k = 5;
    int dp[k+1];
    dp[0]=1;
    for (int i=1; i<=k; i++)
    {
        dp[i]=i*dp[i-1];
        printf("%d : %d\n", i, dp[i]);
    }

    return 0;
}
