#define _CRT_SECURE_NO_WARNINGS 
#include <stdio.h>
int main()
{
	int score[5];
	int i;
	int total = 0;
	int count = 0;
	double avg;


	count = sizeof(score) / sizeof(score[0]);
	for (int i = 0; i < count; i++)
	{
		scanf("%d", &score[i]);
		total += score[i];
		printf("%d", score[i]);
	}
	avg = total / (double)count;
	printf("\n평균: %.1lf", avg);

	return 0;
}
