#include <stdio.h>

struct list {
    int num;
    struct list *next;
};

int main()
{
    struct list a = {10, NULL}, b={20, NULL}, c={30,NULL};
    struct list *head = &a;
    a.next = &b;
    b.next = &c;
    printf("%d\n", head->num);
    printf("%d\n", head->next->num);
    printf("%d\n", head->next->next->num);   

    struct list *current = head;
    while(current != NULL){
        printf("%4d", current->num);
        current = current->next;
    }
    printf("\n");
    return 0;
}
